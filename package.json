{
	"name": "axebot",
	"version": "0.1.0",
	"description": "Bot discord",
	"main": "bot.js",
	"author": "Zaxerone",
	"license": "MIT",
	"dependencies": {
		"chalk": "^3.0.0",
		"discord-akairo": "discord-akairo/discord-akairo",
		"discord.js": "discordjs/discord.js",
		"dotenv": "^8.2.0",
		"ejs": "^3.0.1",
		"express": "^4.17.1",
		"express-session": "^1.17.0",
		"memorystore": "^1.6.1",
		"ms": "^2.1.2",
		"passport": "^0.4.1",
		"passport-discord": "^0.1.3",
		"sqlite3": "^4.1.1"
	},
	"devDependencies": {
		"eslint": "^6.7.2",
		"eslint-config-aqua": "^7.1.2"
	},
	"eslintConfig": {
		"extends": "aqua/node",
		"rules": {
			"linebreak-style": [
				"error",
				"windows"
			],
			"quotes": [
				"warn",
				"double",
				"single"
			],
			"indent": [
				"error",
				2
			],
			"curly": "off"
		}
	}
}
